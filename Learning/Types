# Types

# Integers, strings, booleans, floats, 

is_game_over = True
print (is_game_over)

num_lives = 5
print (num_lives)

percent_health = 0.5
print (percent_health)

player_name = "John"
print (player_name)

print (type(is_game_over))
print (type(num_lives))
print (type(player_name))
print (type(percent_health))

